<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no" />
    <meta name="description" content="" />
    <meta name="author" content="" />
    <link rel="icon" type="image/x-icon" href="~/logo.ico" />

    <title>@ViewData["Title"] - Administration</title>
    <link href="~/css/styles.css" rel="stylesheet" />
    <link href="~/css/main.css" rel="stylesheet" />
    <!-- Bootstrap 5 CSS -->
    <link href="~/lib/bootstrap-5.0.2/dist/css/bootstrap.min.css" rel="stylesheet" crossorigin="anonymous">

    <!-- DataTables CSS (Bootstrap 5 compatible) -->
    <link href="~/css/dataTables.bootstrap5.min.css" rel="stylesheet" crossorigin="anonymous">
    <!-- Font Awesome 6 -->
    <link href="https://cdn.jsdelivr.net/npm/bootstrap-icons/font/bootstrap-icons.css" rel="stylesheet">
    <link href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.0.0-beta3/css/all.min.css" rel="stylesheet">
    <!-- Plugins Css-->
    <link href="https://cdn.jsdelivr.net/npm/bootstrap-datepicker@1.9.0/dist/css/bootstrap-datepicker.min.css" rel="stylesheet">
    @* <link href="~/lib/plugins/datepicker/dist/css/bootstrap-datepicker.min.css" rel="stylesheet" /> *@
    <!-- Select2 CSS -->
    <link href="https://cdn.jsdelivr.net/npm/select2@4.0.13/dist/css/select2.min.css" rel="stylesheet" />

    


    <!-- jQuery (required for DataTables and validation) -->
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <!-- Bootstrap 5 Bundle JS (includes Popper) -->
    <script src="~/lib/bootstrap-5.0.2/dist/js/bootstrap.bundle.min.js" crossorigin="anonymous"></script>

    <!-- DataTables JS (Bootstrap 5 compatible) -->
    <script src="~/js/system/jquery.dataTables.min.js"></script>
    <script src="~/js/system/dataTables.bootstrap5.min.js"></script>
    <!-- Plugins Js -->
    <script src="https://cdnjs.cloudflare.com/ajax/libs/moment.js/2.29.4/moment.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap-datepicker@1.9.0/dist/js/bootstrap-datepicker.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap-datepicker@1.9.0/dist/locales/bootstrap-datepicker.vi.min.js"></script>
    <!-- Select2 JS -->
    <script src="https://cdn.jsdelivr.net/npm/select2@4.0.13/dist/js/select2.min.js"></script>

    <script src="~/lib/plugins/tinymce/tinymce.min.js"></script>

    @* page JS *@
    <script src="~/js/system/tinymce.js"></script>
    <script src="~/js/system/main.js"></script>
    <script src="~/js/menuSideNav.js"></script>
</head>
<body class="sb-nav-fixed">
    <div id="global-spinner" style="display: none;">
        <div class="spinner-border text-primary" role="status">
            <span class="visually-hidden">Loading...</span>
        </div>
    </div>
    @await Component.InvokeAsync("Navigation")
    <div id="layoutSidenav">
        <div id="layoutSidenav_nav">
            <nav class="sb-sidenav accordion sb-sidenav-dark" id="sidenavAccordion">
                @await Component.InvokeAsync("MenuLeft")

            </nav>
        </div>
        <div id="layoutSidenav_content">
            
            <main class="layout-main-content">
                @RenderBody()
            </main>
            <footer class="py-4 bg-light mt-auto">
                <div class="container-fluid">
                    <div class="d-flex align-items-center justify-content-between small">
                        <div class="text-muted">Copyright &copy; Nhien Dentistry 2024</div>
                        <div>
                            <a href="#">Privacy Policy</a>
                            &middot;
                            <a href="#">Terms &amp; Conditions</a>
                        </div>
                    </div>
                </div>
            </footer>
        </div>
    </div>
    <!-- Info Modal -->
    <div class="modal fade" id="infoModal" tabindex="-1" aria-labelledby="infoModalLabel" aria-hidden="true">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-header infoModal-header">
                    <h5 class="modal-title" id="infoModalLabel">Thông báo</h5>
                    <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
                </div>
                <div class="modal-body" id="message-content">
                    Thông báo.
                </div>
                <div class="modal-footer">
                    <button type="button" id="infoModal-close" class="btn btn-secondary" data-bs-dismiss="modal">Đóng</button>
                    <button type="button" id="infoModal-confirm" data-bs-dismiss="modal" class="btn btn-warning">Xác nhận</button>
                </div>
            </div>
        </div>
    </div>

    <script src="~/js/system/scripts.js"></script>
    @RenderSection("Scripts", required: false)
</body>
</html>