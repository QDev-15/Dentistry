        </requestFiltering>
      </security>
      
      <aspNetCore processPath="dotnet" arguments=".\Dentistry.Admin.dll" stdoutLogEnabled="false" stdoutLogFile=".\logs\stdout" hostingModel="inprocess" />
    </system.webServer>
  </location>
</configuration>
